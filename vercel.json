{
    "buildCommand": "cd frontend_staydayz && npm install && ./node_modules/vite/bin/vite.js build",
    "outputDirectory": "frontend_staydayz/dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)", 
            "destination": "/api/index.js"
        }
    ]
}